{
  "name": "@discourse/lint-configs",
  "version": "1.0.0",
  "description": "Shareable lint configs for Discourse core, plugins, and themes",
  "author": "Discourse",
  "license": "MIT",
  "keywords": [
    "discourse"
  ],
  "repository": "https://github.com/discourse/eslint-config-discourse",
  "homepage": "https://github.com/discourse/eslint-config-discourse#readme",
  "module": "index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./eslint": {
      "import": "./eslint.config.js",
      "require": "./eslintrc.cjs"
    },
    "./eslint-theme": {
      "import": "./eslint-theme.js",
      "require": "./eslint-theme.cjs"
    },
    "./prettier": {
      "require": "./.prettierrc.cjs"
    },
    "./template-lint": {
      "import": "./template-lint.config.js",
      "require": "./template-lint.config.cjs"
    }
  },
  "scripts": {
    "build": "pnpm build:template-lint",
    "build:template-lint": "rollup template-lint.config.js --file template-lint.config.cjs --config rollup.config.js",
    "test": "pnpm build && cd ../test && node test.js"
  },
  "dependencies": {
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/parser": "^7.23.0",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@babel/types": "^7.23.0",
    "@typescript-eslint/visitor-keys": "^6.8.0",
    "ember-template-lint": "^5.11.2",
    "eslint": "^8.51.0",
    "eslint-plugin-decorator-position": "^5.0.2",
    "eslint-plugin-discourse-ember": "latest",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-class-members": "^1.19.0",
    "globals": "^13.23.0",
    "hermes-parser": "^0.17.0",
    "prettier": "2.8.8",
    "prettier-plugin-ember-template-tag": "^0.3.2",
    "to-fast-properties": "^3.0.1"
  },
  "devDependencies": {
    "rollup": "^4.1.4"
  }
}
