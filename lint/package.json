{
  "name": "eslint-config-discourse",
  "version": "5.0.0",
  "description": "Shareable eslint config for Discourse and plugins",
  "author": "Joffrey Jaffeux",
  "license": "MIT",
  "keywords": [
    "discourse"
  ],
  "repository": "https://github.com/discourse/eslint-config-discourse",
  "homepage": "https://github.com/discourse/eslint-config-discourse#readme",
  "module": "index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.cj",
      "require": "./index.cjs"
    },
    "./eslint": {
      "import": "./eslint.config.js",
      "require": "./eslint.config.cjs"
    },
    "./prettier": {
      "import": "./prettier.config.js",
      "require": "./prettier.config.cjs"
    },
    "./template-lint": {
      "import": "./template-lint.config.js",
      "require": "./template-lint.config.cjs"
    }
  },
  "scripts": {
    "build": "pnpm build:eslint && pnpm build:prettier && pnpm build:template-lint",
    "build:eslint": "rollup eslint.config.js --file eslint.config.cjs --format cjs",
    "build:prettier": "rollup prettier.config.js --file prettier.config.cjs --format cjs",
    "build:template-lint": "rollup template-lint.config.js --file template-lint.config.cjs --format cjs"
  },
  "dependencies": {
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "ember-template-imports": "^3.4.2",
    "ember-template-lint": "^5.11.2",
    "eslint": "^8.51.0",
    "eslint-plugin-decorator-position": "^5.0.2",
    "eslint-plugin-discourse-ember": "latest",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-class-members": "^1.19.0",
    "prettier": "3.0.3",
    "prettier-plugin-ember-template-tag": "^1.1.0"
  },
  "devDependencies": {
    "rollup": "^4.0.2"
  }
}
